const log = require('logToConsole');
const JSON = require('JSON');
const injectScript = require('injectScript');
const getUrl = require('getUrl');
const encodeUriComponent = require('encodeUriComponent');

const pageURL = getUrl();

log('pageURL', pageURL);

const isDev = pageURL.search('https://dev-sas-web.site/') === 0;
log('isDev', isDev);


const domain = isDev ? 'dev-sas.site' : 'trinitymedia.ai';
log('domain', domain);

const ENDPOINT = 'https://' + domain + '/player/trinity/';


let selectorValue;
const isSelectorUsed = data.placeholderSelector && data.placeholderPosition;

if (isSelectorUsed) {
  selectorValue = JSON.stringify({
    selector: data.placeholderSelector,
    position: data.placeholderPosition
  });
}

log('ENDPOINT', ENDPOINT);

let url = ENDPOINT + data.unitId + '/?pageURL=' + encodeUriComponent(pageURL) + '&integrationType=gtm';
if (isSelectorUsed) url += '&placeholderSelector=' + encodeUriComponent(selectorValue);

log('URL', url);




injectScript(url, () => {
  log('Trinity player GTM', 'successfully loaded');
  data.gtmOnSuccess();
}, (error) => {
    log('Trinity player GTM', 'error to loaded', error);
    data.gtmOnFailure();
});


